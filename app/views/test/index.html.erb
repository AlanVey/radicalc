<h3>Listing Tests</h3>

<table class="table">
  <thead>
    <tr>
      <th>Title</th>
      <th>Kind</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tests.each do |test| %>
      <tr>
        <td><%= link_to test.title, show_test_path(test_id: test.id) %></td>
        <td><%= test.kind %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br><br>
<%= link_to 'New Test', new_test_path %><br>
<%= link_to 'Back', questions_path(kind:'General') %>
